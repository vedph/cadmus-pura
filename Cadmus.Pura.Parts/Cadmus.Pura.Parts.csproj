<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<Nullable>enable</Nullable>
    <TargetFramework>net7.0</TargetFramework>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus models for PURA.</Description>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageTags>Cadmus;PURA</PackageTags>
    <Version>4.0.1</Version>
    <FileVersion>4.0.1</FileVersion>
    <AssemblyVersion>4.0.1</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Graph.mysql" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\Graph.mysql" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="5.0.0" />
    <PackageReference Include="Cadmus.Seed.General.Parts" Version="3.0.2" />
  </ItemGroup>
</Project>
